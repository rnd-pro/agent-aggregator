{
  "name": "agent-aggregator",
  "version": "1.0.3",
  "description": "MCP server that aggregates tools from multiple MCP servers and provides unified access to AI agents",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "agent-aggregator": "bin/agent-aggregator.js",
    "agent-aggregator-setup": "bin/setup.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "node tests/integration.test.js",
    "test:server": "node scripts/test-server.js",
    "test:ai": "node scripts/test-ai-models.js",
    "test:ai:multiple": "node scripts/test-ai-models.js --multiple",
    "dev": "node --watch src/index.js",
    "setup": "node bin/setup.js",
    "install-cursor": "node bin/install-cursor.js"
  },
  "files": [
    "src/",
    "bin/",
    "config/",
    "start-mcp.js",
    "README.md"
  ],
  "keywords": [
    "mcp",
    "server",
    "aggregator",
    "proxy",
    "ai",
    "agents",
    "cursor",
    "qwen",
    "claude"
  ],
  "author": "RND-PRO",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RND-PRO/agent-aggregator.git"
  },
  "homepage": "https://github.com/RND-PRO/agent-aggregator#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
