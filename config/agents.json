{
  "agents": [
    {
      "name": "filesystem",
      "type": "mcp",
      "enabled": true,
      "description": "File system operations server for local file access",
      "connection": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"],
        "env": {}
      },
      "model": {
        "provider": "openrouter",
        "name": "qwen/qwen3-coder:free",
        "apiKey": "${OPENROUTER_API_KEY}"
      }
    },
    {
      "name": "everything",
      "type": "mcp",
      "enabled": true,
      "description": "Everything server for testing all MCP features",
      "connection": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-everything"],
        "env": {}
      },
      "model": {
        "provider": "openrouter",
        "name": "qwen/qwen3-coder:free",
        "apiKey": "${OPENROUTER_API_KEY}"
      }
    },
    {
      "name": "claude-code-mcp",
      "type": "mcp",
      "enabled": true,
      "description": "Claude Code MCP server wrapper (requires Claude CLI)",
      "connection": {
        "command": "npx",
        "args": ["-y", "@kunihiros/claude-code-mcp"],
        "env": {
          "CLAUDE_BIN": "/usr/local/bin/claude",
          "LOG_LEVEL": "info"
        }
      },
      "model": {
        "provider": "openrouter",
        "name": "qwen/qwen3-coder:free",
        "apiKey": "${OPENROUTER_API_KEY}"
      }
    }
  ],
  "aggregator": {
    "timeout": 30000,
    "retryAttempts": 3,
    "retryDelay": 1000,
    "concurrentConnections": 3
  },
  "defaults": {
    "model": {
      "provider": "openrouter",
      "name": "qwen/qwen3-coder:free",
      "fallback": "openai/gpt-4o-mini",
      "apiKey": "${OPENROUTER_API_KEY}",
      "baseUrl": "https://openrouter.ai/api/v1"
    }
  }
}